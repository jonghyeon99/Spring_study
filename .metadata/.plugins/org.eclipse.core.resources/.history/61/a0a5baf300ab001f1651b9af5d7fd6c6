package net.scit.spring1.controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;

@Controller
public class SecondController {
	
	@GetMapping("/param")
	public String param() {
		return "param";
	}
	
	@GetMapping("/info")
	public String info() {
		return "info";
	}
}
